<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ijourney.conbowcontrol">

    <application
        android:name=".application.MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.ijourney.conbowcontrol.MainActivity"
            android:screenOrientation="landscape">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".old.FeaturesActivity"
            android:screenOrientation="landscape" />

        <activity
            android:name=".old.OldChatActivity"
            android:screenOrientation="landscape">

        </activity>

        <activity
            android:name="com.ijourney.conbowcontrol.ChatActivity"
            android:screenOrientation="landscape" />
    </application>

</manifest>
